{
  "name": "issue-collab",
  "version": "1.8.0",
  "description": "Tool for finding open source projects on GitHub",
  "bugs": {
    "url": "https://github.com/trybick/issue-collab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trybick/issue-collab"
  },
  "keywords": [
    "github",
    "issues",
    "search",
    "open-source"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:fix": "eslint --fix --ext .jsx,.js ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.9.1",
    "moment": "^2.22.2",
    "node-sass": "^4.14.1",
    "react": "^16.6.0",
    "react-dark-mode-toggle": "0.0.10",
    "react-dom": "^16.6.0",
    "react-ga4": "^2.1.0",
    "react-scripts": "^2.1.3",
    "react-spinners": "^0.9.0",
    "react-toggle": "^4.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^1.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{jsx,js}": "eslint --cache --fix"
  }
}
